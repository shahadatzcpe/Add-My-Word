{
  "manifest_version": 2,

  "name": "Add to my word",
  "description": "Add to my word list",
  "version": "1.0",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },

  "browser_action": {
    "default_icon": "icon_16.png"
  },

  "background": {
    "scripts": [
      "jquery-3.4.1.min.js",
      "eventPage.js"
    ],
    "persistent": false
  },

  "permissions": [
      "storage",
      "notifications",
      "contextMenus",
      "tabs"
  ]
}
